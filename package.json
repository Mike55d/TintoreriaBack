{
  "name": "adv-tickets-backend",
  "private": true,
  "version": "2.0.2",
  "description": "ADV Tickets Backend",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:prod": "nest build --webpack --webpackPath webpack-hmr.config.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "typeorm-ts-node-commonjs"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.4",
    "@faker-js/faker": "^8.0.1",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^8.1.1",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.1.1",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.4.3",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.1.1",
    "@nestjs/platform-fastify": "^8.4.3",
    "@nestjs/platform-socket.io": "^8.4.6",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.4.6",
    "@socket.io/redis-adapter": "^7.2.0",
    "@types/cron": "^2.0.1",
    "ajv-dist": "^8.11.0",
    "axios": "^1.6.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.1",
    "deep-diff": "^1.0.2",
    "dotenv": "^16.0.1",
    "electron-log": "^4.4.8",
    "express": "^4.17.3",
    "firebase-admin": "^10.3.0",
    "googleapis": "^105.0.0",
    "i18n": "^0.15.0",
    "jwt-simple": "^0.5.6",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nest-winston": "^1.7.0",
    "nestjs-i18n": "^9.1.5",
    "node-addon-api": "^1.3.0",
    "nodemailer": "^6.7.7",
    "passport": "^0.6.0",
    "passport-google-oidc": "^0.1.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "read-excel-file": "^5.6.1",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "secure-remote-password": "^0.3.1",
    "shelljs": "^0.8.5",
    "socket.io": "^4.5.1",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "0.3.6",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.8.1",
    "xlsx-template": "^1.4.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^8.1.3",
    "@nestjs/schematics": "^8.0.4",
    "@nestjs/testing": "^8.1.1",
    "@types/deep-diff": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.5",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest": "^27.3.0",
    "node-gyp": "^8.3.0",
    "prettier": "^2.4.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.4",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/"
    ]
  }
}
